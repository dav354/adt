      "targets": [
        {
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE \n    WHEN expenses_to_eur < 20000 THEN 'Low Budget (<20k)'\n    WHEN expenses_to_eur < 100000 THEN 'Mid Budget (20k-100k)'\n    WHEN expenses_to_eur < 500000 THEN 'High Budget (100k-500k)'\n    ELSE 'Whales (>500k)'\n  END AS budget_tier,\n  COUNT(*) AS value\nFROM financial_expenses\nWHERE expenses_to_eur IS NOT NULL\nGROUP BY budget_tier\nORDER BY value DESC;",
          "refId": "A"
        }
      ],
      "title": "Spending Power Distribution",
      "type": "piechart"
