      "pluginVersion": "12.2.2",
      "targets": [
        {
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    rank as \"Rang\",\n    name as \"Organisation\",\n    expenses as \"Budget (EUR)\"\nFROM (\n    SELECT\n        COALESCE(li.name_text, li.company_name) AS name,\n        fe.expenses_to_eur AS expenses,\n        RANK() OVER (\n            ORDER BY fe.expenses_to_eur DESC NULLS LAST\n        ) AS rank\n    FROM financial_expenses fe\n    JOIN lobbyist_identity li ON fe.entry_id = li.entry_id\n) AS ranked_stats\nWHERE rank <= 20\nORDER BY rank ASC;",
          "refId": "A"
