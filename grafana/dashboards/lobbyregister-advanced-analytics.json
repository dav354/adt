      "targets": [
        {
          "dataset": "lobby",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "lobbyregister"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  li.name_text AS name,\n  (fe.expenses_to_eur / GREATEST(emp.employee_fte, 1.0))::double precision AS spend_per_head\nFROM register_entry re\nJOIN lobbyist_identity li ON re.id = li.entry_id\nJOIN financial_expenses fe ON re.id = fe.entry_id\nJOIN employees_involved emp ON re.id = emp.entry_id\nWHERE fe.expenses_to_eur IS NOT NULL \n  AND emp.employee_fte > 0\nORDER BY spend_per_head DESC\nLIMIT 15;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Top Lobbying Efficiency (Spend per Head)",
      "type": "barchart"
    },
