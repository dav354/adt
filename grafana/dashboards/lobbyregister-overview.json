      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "lobbyregister"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT metric,\n       value\nFROM (\n  SELECT 'Entries with listed clients' AS metric,\n         count(*)::double precision AS value\n  FROM client_identity\n  WHERE clients_present\n  UNION ALL\n  SELECT 'Total client organisations' AS metric,\n         count(*)::double precision AS value\n  FROM client_org\n  UNION ALL\n  SELECT 'Total client persons' AS metric,\n         count(*)::double precision AS value\n  FROM client_person\n  UNION ALL\n  SELECT 'Average clients per entry' AS metric,\n         AVG(COALESCE(clients_count, 0))::double precision AS value\n  FROM client_identity\n) metrics\nORDER BY metric;",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "",
          "timeColumn": "",
          "where": []
        }
      ]
