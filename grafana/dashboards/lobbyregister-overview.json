      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "lobbyregister"
          },
          "format": "table",
          "group": [],
          "metricColumn": "field_label",
          "rawQuery": true,
          "rawSql": "SELECT\n  COALESCE(cl.de, cl.en, foi.field_of_interest_text, 'Unspecified') AS field_label,\n  count(DISTINCT ai.entry_id)::double precision AS registrations,\n  count(*)::double precision AS mentions\nFROM field_of_interest foi\nJOIN activities_interests ai ON ai.id = foi.activities_id\nLEFT JOIN code_label cl ON cl.id = foi.label_id\nGROUP BY field_label\nORDER BY mentions DESC\nLIMIT 10;",
          "select": [
            [
              {
                "params": [
                  "mentions"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "",
          "timeColumn": "",
          "where": []
        }
      ]
