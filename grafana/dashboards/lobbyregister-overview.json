      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "lobbyregister"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  re.register_number,\n  fe.related_year_start,\n  fe.related_year_end,\n  COALESCE(fe.expenses_from_eur, 0) AS expenses_from_eur,\n  COALESCE(fe.expenses_to_eur, fe.expenses_from_eur, 0) AS expenses_to_eur,\n  ((COALESCE(fe.expenses_from_eur, fe.expenses_to_eur, 0) + COALESCE(fe.expenses_to_eur, fe.expenses_from_eur, 0)) / 2.0)::double precision AS midpoint_estimate_eur\nFROM financial_expenses fe\nJOIN register_entry re ON re.id = fe.entry_id\nWHERE fe.expenses_from_eur IS NOT NULL OR fe.expenses_to_eur IS NOT NULL\nORDER BY COALESCE(fe.expenses_to_eur, fe.expenses_from_eur, 0) DESC\nLIMIT 20;",
          "select": [
            [
              {
                "params": [
                  "expenses_to_eur"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "",
